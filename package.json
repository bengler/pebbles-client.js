{
  "name": "@bengler/pebbles-client",
  "version": "3.0.0",
  "description": "Multi-environment JavaScript client for pebbles services",
  "main": "index.js",
  "browser": {
    "http": "http-browserify"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/bengler/pebbles-client.js.git"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "postpublish": "rimraf lib",
    "compile": "babel src --out-dir lib",
    "compile:watch": "npm run compile -- --watch",
    "mocha": "mocha",
    "lint": "eslint .",
    "test": "npm run mocha && npm run lint -- --quiet"
  },
  "author": "Bjørge Næss",
  "license": "MIT",
  "dependencies": {
    "deep-extend": "^0.4.1",
    "duplexify": "^3.5.0",
    "http-browserify": "git://github.com/bjoerge/http-browserify#fix-withcredentials-bug",
    "inherits": "~2.0.3",
    "pumpify": "^1.3.5",
    "split2": "^2.1.0",
    "through": "^2.3.8",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "concat-stream": "^1.5.2",
    "eslint": "^3.12.0",
    "eslint-config-bengler": "3.0.1",
    "mocha": "^3.2.0",
    "mock": "~0.1.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "through2": "^2.0.3"
  },
  "publishConfig": {
    "registry": "https://npm.bengler.no"
  }
}
